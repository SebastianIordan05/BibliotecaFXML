<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.biblioteca.bibliotecafxml.EndprestitoController">
   <children>
      <ChoiceBox fx:id="chbLibro" layoutX="110.0" layoutY="175.0" prefWidth="150.0" onAction="#getSelectedBook"  />
      <ChoiceBox fx:id="chbCausale" layoutX="340.0" layoutY="175.0" prefWidth="150.0" onAction="#getSelectedCausale"  />
      <Label layoutX="110.0" layoutY="149.0" text="Libro" />
      <Label layoutX="340.0" layoutY="149.0" text="Causale" />
      <Button fx:id="btnBackToOption" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToPrimary" text="&lt;-" />
      <Button fx:id="btnEndPrestito" layoutX="260.0" layoutY="292.0" mnemonicParsing="false" onAction="#endPrestito" text="end prestito" />
      <Label layoutX="258.0" layoutY="53.0" text="termina prestito" />
   </children>
</AnchorPane>
